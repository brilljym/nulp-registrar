# Add these to your .env file for LOCAL development (thermal printing enabled)

# Printing Configuration
PRINT_LOCAL_ENABLED=true
POS_PRINTER_NAME=POS-58
PRINT_POLLING_INTERVAL=5
PRINT_MAX_RETRIES=3
PRINT_BATCH_SIZE=10
PRINT_CLEANUP_DAYS=30

# Remote database settings (not needed for local)
PRINT_REMOTE_DB_ENABLED=false